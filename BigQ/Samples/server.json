      "PFXCertPassword": "password"
   },
   "WebsocketServer": {
      "Enable": false,
      "IP": "0.0.0.0",
      "Port": 8002
   },
   "WebsocketSSLServer": {
      "Enable": false,
      "IP": "0.0.0.0",
      "Port": 8003,
      "PFXCertFile": "server.pfx",
      "PFXCertPassword": "password"
   },
   "ServerChannels": [
      {
         "Broadcast": 1,
         "Multicast": 0,
         "Unicast": 0,
         "ChannelName": "Default server channel",
         "Private": 0
      }
   ]
}
