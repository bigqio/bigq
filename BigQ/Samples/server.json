{
   "Version": "1.0.0",
   "AcceptInvalidSSLCerts": true,
   "Files": {
      "UsersFile": "users.json",
      "PermissionsFile": "permissions.json"
   },
   "Heartbeat": {
      "Enable": false,
      "IntervalMs": 1000,
      "MaxFailures": 5
   },
   "Notification": {
